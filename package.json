{
  "name": "jason-fe-portfolio",
  "version": "1.0.0",
  "description": "Modern static portfolio website built with Next.js, TypeScript, and TailwindCSS featuring markdown-based content management",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "next dev",
    "build": "npm run build:search-index && next build",
    "build:search-index": "tsx src/lib/search/build-index.ts",
    "export": "npm run build",
    "start": "next start",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "type-check": "tsc --noEmit",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "prepare": "husky"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sonbuiGFD/jason-fe-portfolio.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/sonbuiGFD/jason-fe-portfolio/issues"
  },
  "homepage": "https://sonbuiGFD.github.io/jason-fe-portfolio",
  "dependencies": {
    "@tailwindcss/postcss": "^4.1.16",
    "@types/hast": "^3.0.4",
    "@types/react-syntax-highlighter": "^15.5.13",
    "fuse.js": "^7.1.0",
    "gray-matter": "^4.0.3",
    "motion": "^12.23.24",
    "next": "^16.0.1",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "react-syntax-highlighter": "^16.1.0",
    "rehype-highlight": "^7.0.2",
    "rehype-raw": "^7.0.0",
    "rehype-stringify": "^10.0.1",
    "remark": "^15.0.1",
    "remark-gfm": "^4.0.1",
    "remark-html": "^16.0.1",
    "remark-prism": "^1.3.6",
    "sass": "^1.93.2",
    "tailwindcss": "4.1"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.3.1",
    "@eslint/js": "^9.38.0",
    "@svgr/webpack": "^8.1.0",
    "@types/eslint-plugin-jsx-a11y": "^6.10.1",
    "@types/node": "^24.9.2",
    "@types/react": "^19.2.2",
    "@types/react-dom": "^19.2.2",
    "@typescript-eslint/eslint-plugin": "^8.46.2",
    "@typescript-eslint/parser": "^8.46.2",
    "autoprefixer": "^10.4.21",
    "eslint": "^9.38.0",
    "eslint-config-next": "^16.0.1",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^7.0.1",
    "husky": "^9.1.7",
    "lint-staged": "^16.2.6",
    "postcss": "^8.5.6",
    "prettier": "^3.6.2",
    "tsx": "^4.20.6",
    "typescript": "5.9",
    "typescript-eslint": "^8.46.2"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{json,md,yml,yaml,scss,css}": [
      "prettier --write"
    ]
  }
}
